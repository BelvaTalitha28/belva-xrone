public class Tugas2Seharusnya {

    public static void main(String[] args) {
        int bilangan[] = new int[5];
        bilangan[0] = 1;
        bilangan[1] = 2;
        bilangan[2] = 3;
        bilangan[3] = 4;
        bilangan[4] = 5;
        /* bilangan[5]=6;
        bilangan[6]=7;
        bilangan[7]=8;*/

        for (int i = 0; i < bilangan.length; i++) {
            System.out.println(i);

        }
    }

}
