public class Tugas1Seharusnya {
     public static void main(String[] args) {
        System.out.println("==tugas1==");
        int bilangan[]=new int[5];
        bilangan[0]=1;
        bilangan[1]=2;
        bilangan[2]=3;
        bilangan[3]=4;
        bilangan[4]=5;
         System.out.println(bilangan[4]);
        //tidak bisa jika System.out.println(bilangan[6]);
    }
}
